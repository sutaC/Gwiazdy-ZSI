<!-- prettier-ignore -->
<img  
    src="<%= photo.src || ("/uploads/" + photo.local) %>" 
    alt="Gwiazda ZSI"
    class="imageDisplay"
    loading="lazy"
    hx-boost="true"
    hx-on:click="window.open(this.src, '_blank')"
    <% if (photo.src && photo.local) { %>
        onerror="this.onerror = null; this.src = '<%= '/uploads/' + photo.local %>'"
    <% } %>
/>
